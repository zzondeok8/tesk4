#include <stdio.h>
#include <string.h>
#include <cs50.h>

int main (void)
{
    int number [6][5] = {
    {1,2,3,4,5},
    {5,4,3,2,1},
    {1,4,2,5,8},
    {2,5,4,3,1},
    {1,1,1,3,2},
    {2,1,1,3,1}
};
  
    int temp;
    int str [] = {12345, 54321, 14258,25431,11132,21131};
      
{ 
    for (int s=0; s<5; s=s+2)
    printf ("Compare %i and %i\n",str[s],str[s+1]);
for ( int k=0; k<6; k++)
  
 
  for (int i=0; i < 5; i++)
  {
    for (int j=0; j < 5 - i - 1; j++ )
    {
      if (number[k][j] > number[k][j+1])
      {
        temp = number[k][j];
        number[k][j] = number[k][j+1];
        number[k][j+1] = temp;
      }
    }
  }
 }
 
   
   
    {   for(int i=0;i<5;i=i+2)
        
        if (number[i][0]==number[i+1][0]&&number[i][1]==number[i+1][1]&&number[i][2]==number[i+1][2]&&number[i][3]==number[i+1][3]&&number[i][4]==number[i+1][4])
        {
            printf("TRUE\n");
        }
        else
        {
            printf("FALSE\n");
        }
        }
        
     return 0;
    }
